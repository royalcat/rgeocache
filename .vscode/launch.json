{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Generate database",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "cwd": "${workspaceFolder}/bin",
            "args": [
                "generate",
                "--pprof.profile",
                "--pprof.heap",
                "--pprof.listen",
                ":8080",
                "-l",
                "ru",
                "-i",
                "cache_build/maps/europe/belarus-latest.osm.pbf",
                "-i",
                "cache_build/maps/europe/ukraine-latest.osm.pbf",
                "-i",
                "cache_build/maps/asia/azerbaijan-latest.osm.pbf",
                "--points",
                "points_data.gob",
            ]
        },
        {
            "name": "Serve",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "cwd": "${workspaceFolder}/bin",
            "args": [
                "serve",
                "--points",
                // "${workspaceFolder}/cache_build/points_data.gob",
                // "points_data.gob",
                "post-cis_ru_points.gob",
            ]
        },
    ]
}